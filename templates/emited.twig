<div class="tab-pane fade" id="facturacion-data" role="tabpanel">
	<div class="card w-100">
		<div class="card-header">
			Comprobantes emitidos
		</div>
		<div class="card-body  pl-4">
			<form method="POST" id="idFormEmited">

				<div class="d-flex justify-content-between" >
					<label>Enviar sobre a receptor electrónico (xml)</label>
					<button id="" class="btn" type="button" onclick="addRowToInsertCfeData()" ><i class="fas fa-plus-circle"></i></button>
				</div>
				<div id="rowsToInsertCfeData" >
					<div class="form-row" >
						<div class="form__group field w-25 m-2">
							<select class="form__field" id="idSelectTipoCfeSendXml" name="nameSelectTipoCfeSendXml0">
								<option value="101" >E-Ticket</option>
								<option value="102" >N. crédito E-Ticket</option>
								<option value="103" >N. débito E-Ticket</option>
								<option value="111" selected>E-Factura</option>
								<option value="112" >N. crédito E-Factura</option>
								<option value="113" >N. débito E-Factura</option>
								<option value="121" >E-Factura Exportación</option>
								<option value="122" >N. crédito E-Factura Exportación</option>
								<option value="123" >N. débito E-Factura Exportación</option>
								<option value="124" >E-Remito de Exportación</option>
								<option value="131" >E-Ticket Venta por Cuenta Ajena</option>
								<option value="132" >N. crédito E-Ticket Venta por Cuenta Ajena</option>
								<option value="133" >N. débito E-Ticket Venta por Cuenta Ajena</option>
								<option value="141" >E-Factura Venta por Cuenta Ajena</option>
								<option value="142" >N. crédito E-Factura Venta por Cuenta Ajena</option>
								<option value="143" >N. débito E-Factura Venta por Cuenta Ajena</option>
								<option value="151" >E-Boleta de entrada</option>
								<option value="152" >N. crédito E-Boleta de entrada</option>
								<option value="153" >N. débito E-Boleta de entrada</option>
								<option value="181" >E-Remito</option>
								<option value="182" >E-Resguardo</option>
							</select>
						</div>
						<div class="form__group field m-2">
							<input type="text" id="idInputSerieCfeSendXml" name="nameInputSerieCfeSendXml0" class="form__field input_uppercase" placeholder="Serie" required/>
						</div>
						<div class="form__group field m-2">
							<input type="text" id="idInputNumeroCfeSendXml" name="nameInputNumeroCfeSendXml0" class="form__field" placeholder="Número" required/>
						</div>
					</div>
				</div>
				<br><br>
				<label>Copiar a</label>
				<div class="form-row" >
					<div class="form__group field w-100">
						<input type="text" id="idInputMailsCopySendXml" name="nameInputMailsCopySendXml" class="form__field" placeholder="Correo" />
					</div>
				</div>
				<br><br>
				<div class="form-row d-flex flex-row-reverse" >
					<button id="buttonSubmitEmitedForm" class="btn btn-success" type="submit" >Envíar</button>
				</div>
			</form>
		</div>
	</div>
</div>