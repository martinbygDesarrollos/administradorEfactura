<!DOCTYPE html>
<html lang="es">
<head>
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<link rel="icon" type="image/png" sizes="32x32" href="{{ path_for('Start') }}img/favicon.ico">
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.6.0/css/bootstrap.min.css" integrity="sha512-P5MgMn1jBN01asBgU0z60Qk4QxiXo86+wlFahKrsQf37c9cro517WzVSPPV1tDKzhku2iJ2FVgL67wG03SGnNA==" crossorigin="anonymous" referrerpolicy="no-referrer" />

	<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js" integrity="sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4" crossorigin="anonymous"></script>

	<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.js" integrity="sha512-n/4gHW3atM3QqRcbCn6ewmpxcLAHGaDjpEBu4xZd47N0W2oQ+6q7oc3PXstrJYXcbNU1OHdQ1T7pAP+gi5Yu8g==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.6.0/js/bootstrap.min.js" integrity="sha512-XKa9Hemdy1Ui3KSGgJdgMyYlUg1gM+QhL6cnlyTe2qzMCYm4nAZ1PsVerQzTTXzonUR+dmswHqgJPuwCq1MaAg==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
	<link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
	<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.1/css/all.css" integrity="sha384-50oBUHEmvpQ+1lW4y57PTFmhCaXp0ML5d60M1M7uH2+nqUivzIebhndOJK28anvf" crossorigin="anonymous">


	<link rel="stylesheet" type="text/css" href="{{ path_for('Start')}}styles/stylesDefault/globalStyles.css{{ version }}">
	<link rel="stylesheet" type="text/css" href="{{ path_for('Start')}}styles/stylesDefault/slider.css{{ version }}">
	<link rel="stylesheet" type="text/css" href="{{ path_for('Start')}}styles/stylesDefault/toggle.css{{ version }}">
	<link rel="stylesheet" type="text/css" href="{{ path_for('Start')}}styles/stylesDefault/containerTable.css{{ version }}">
	<link rel="stylesheet" type="text/css" href="{{ path_for('Start')}}styles/inputsForm.css{{ version }}">
	<link rel="stylesheet" type="text/css" href="{{ path_for('Start')}}styles/simpleMessage.css{{ version }}">
	<link rel="stylesheet" type="text/css" href="{{ path_for('Start')}}styles/buttonUp.css{{ version }}">
	<link rel="stylesheet" type="text/css" href="{{ path_for('Start')}}styles/dropdownFilter.css{{ version }}">
	<title>{% block title %}Administrador{% endblock %}</title>
</head>
<body class="d-flex flex-column" style="background-image: url('{{ path_for('Start')}}img/backGround.png'); background-repeat: no-repeat; background-attachment: fixed; background-size: cover;">
	<div class="page-contentClass page-content" id="content">
		<div class="d-flex justify-content-between" id="idDivHorizontalNav">
			<div>
				<button id="sidebarCollapse" type="button" class="btn background-template-color text-template-color m-2"></button>
				<a type="button" href="{{ path_for('Start') }}" class="btn background-template-color text-template-color m-2"><i class="fas fa-home"></i></a>
			</div>
			{% if mailUserLogued %}
			<div class="d-flex">
				<h6 class="text-template-background font-italic mt-3 notShowMobile">{{mailUserLogued}}</h6>
				<a href="{{path_for('SignOut')}}" class="btn btn-danger text-white shadow-sm m-2" data-toggle="tooltip" data-placement="top" title="Cerrar sesión"><i class="fas fa-sign-out-alt"></i></a>
			</div>
			{%else%}
			<a href="{{path_for('SignIn')}}" class="btn btn-success text-white shadow-sm m-2" data-toggle="tooltip" data-placement="top" title="Iniciar sesión"><i class="fas fa-sign-in-alt"></i></a>
			{%endif%}
		</div>
		<div class="container-fluid">
			<div class="row bg-light justify-content-center">
				{% block content %}
				{% endblock %}
			</div>
		</div>
	</div>
	{% include("progressBar.twig") %}
	{% include("slider.twig") %}
	{% include("footer.twig")%}
	<div>
		<button class="btn btn-sm shadow-lg buttonUp" id="buttonUp" onclick="backInit()" title="Ir al inicio"><i class="fas fa-arrow-up"></i></button>
	</div>
	<div id="snackbar" class="fade">
		<button type="button" class="close" accesskey="x" aria-label="Close" id="snackbarButton">
			<span aria-hidden="true">&times;</span>
		</button>
		<div id="snackbarText"></div>
	</div>
	<div id="modalResponse" class="modal" tabindex="-1" role="dialog">
		<div class="modal-dialog" role="document">
			<div class="modal-content">
				<div class="modal-header" id="modalColourResponse">
					<button type="button" class="close" data-dismiss="modal" accesskey="x" aria-label="Close">
						<span aria-hidden="true">&times;</span>
					</button>
				</div>
				<div class="modal-body">
					<p id="modalMessageResponse" class="text-center"></p>
				</div>
				<div class="modal-footer">
					<button id="modalButtonResponseCancel" type="button" class="btn btn-light" onclick="$('#modalResponse').modal('hide')"></button>
					<button id="modalButtonResponse" type="button" class="btn btn-success"></button>
				</div>
			</div>
		</div>
	</div>
	<script type="text/javascript" src="{{ path_for('Start') }}scripts/base.js{{ version }}"></script>
	<script type="text/javascript" src="{{ path_for('Start') }}scripts/sendPost.js{{ version }}"></script>
	<script type="text/javascript" src="{{ path_for('Start') }}scripts/utils.js{{ version }}"></script>
	<script type="text/javascript">
		console.log("calcular opcion del menu activada");
		if ( window.location.href.includes("cliente") ){
			$("#nav-link-companie").removeClass("active");
			$("#nav-link-partner").removeClass("active");
			$("#nav-link-settings").removeClass("active");
			$("#nav-link-home").removeClass("active");
			$("#nav-link-customer").addClass("active");
		}
		else if ( window.location.href.includes("socios") ){
			$("#nav-link-companie").removeClass("active");
			$("#nav-link-partner").addClass("active");
			$("#nav-link-settings").removeClass("active");
			$("#nav-link-home").removeClass("active");
			$("#nav-link-customer").removeClass("active");
		}
		else if ( window.location.href.includes("configuracion") ){
			$("#nav-link-companie").removeClass("active");
			$("#nav-link-partner").removeClass("active");
			$("#nav-link-settings").addClass("active");
			$("#nav-link-home").removeClass("active");
			$("#nav-link-customer").removeClass("active");
		}
		else if ( window.location.href.includes("iniciar-sesion") ){
			$("#nav-link-companie").removeClass("active");
			$("#nav-link-partner").removeClass("active");
			$("#nav-link-settings").removeClass("active");
			$("#nav-link-customer").removeClass("active");
			$("#nav-link-home").addClass("active");
		}
		else{
			$("#nav-link-companie").addClass("active");
			$("#nav-link-partner").removeClass("active");
			$("#nav-link-settings").removeClass("active");
			$("#nav-link-customer").removeClass("active");
			$("#nav-link-home").removeClass("active");
		}

	</script>
</body>
</html>